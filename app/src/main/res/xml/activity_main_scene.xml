<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint motion:layout_editor_absoluteY="6dp" android:layout_height="wrap_content"
                    motion:layout_editor_absoluteX="325dp" android:layout_width="wrap_content"
                    android:id="@+id/getCookieBtn"/>
        <Constraint motion:layout_editor_absoluteY="6dp" android:layout_height="wrap_content"
                    motion:layout_editor_absoluteX="212dp" android:layout_width="wrap_content"
                    android:id="@+id/inputBtn"/>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>